{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Category",
  "type": "object",
  "properties": {
    "category": {
      "type": "object",
      "properties": {
        "id": { "type": "string" },
        "name": { "type": "string" },
        "description": { "type": ["string","null"] },
        "created_at": { "type": "string" },
        "auto_created": { "type": "boolean" },
        "parent_id": { "type": ["string","null"] }
      },
      "required": ["id","name","created_at","auto_created"]
    }
  },
  "required": ["category"]
}